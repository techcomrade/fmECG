    "@types/node": "^22.5.5",
    "mysql2": "^3.11.3",
    "nodemon": "^3.1.5",
    "nswag": "^14.1.0",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@nestjs/core": "^10.4.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/sequelize": "^10.0.1",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
